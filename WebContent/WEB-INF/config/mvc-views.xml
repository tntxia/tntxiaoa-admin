<?xml version="1.0" encoding="UTF-8"?>
<mvcList>
	<mvcView url="index.mvc">
		<title>OA管理</title>
		<isWholePage>true</isWholePage>
		<charset>UTF-8</charset>
		<scripts>
			<script>
				<src>/static/lib/web-install/js/web-install.js</src>
				<resource-path>/oa_admin_static</resource-path>
			</script>
		</scripts>
		<templates>
			<template>index.ftl</template>
		</templates>
	</mvcView>

	<mvcView url="login.mvc">
		<title>用户登录</title>
		<isWholePage>true</isWholePage>
		<stylesheets>
			<stylesheet>lib/bootstrap/css/bootstrap.css</stylesheet>
			<stylesheet>css/login.css</stylesheet>
		</stylesheets>
		<scripts>
			<script>/static/lib/jquery/jquery.js</script>
			<script>js/jquery.blockUI.js</script>
			<script>/static/lib/vue/vue.js</script>
			<script>lib/bootstrap/js/bootstrap.js</script>
			<script>js/login.js</script>
		</scripts>
		<templates>
			<template>login.ftl</template>
		</templates>
	</mvcView>

	<mvcView url="choose.mvc">
		<title>选择项目</title>
		<isWholePage>true</isWholePage>
		<stylesheets>
			<stylesheet>lib/bootstrap/css/bootstrap.css</stylesheet>
			<stylesheet>css/login.css</stylesheet>
		</stylesheets>
		<scripts>
			<script>js/jquery.js</script>
			<script>js/jquery.blockUI.js</script>
			<script>/static/lib/vue/vue.js</script>
			<script>lib/bootstrap/js/bootstrap.js</script>
			<script>js/choose.js</script>
		</scripts>
		<templates>
			<template>choose.ftl</template>
		</templates>
	</mvcView>

</mvcList>